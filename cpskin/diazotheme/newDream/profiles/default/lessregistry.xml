<?xml version="1.0"?>
<object name="portal_less">

 <!-- Deactivate CPSkin theme less files -->

 <stylesheet title="CPSkin LESS Variables"
    id="++theme++cpskin.theme.base/less/variables.less"
    enabled="0" />

 <stylesheet title="CPSkin LESS Styles"
    id="++theme++cpskin.theme.base/less/styles.less"
    enabled="0" />

 <stylesheet title="CPSkin LESS Contact"
    id="++theme++cpskin.theme.base/less/contact.less"
    enabled="0" />

 <stylesheet title="CPSkin LESS Mobile Styles"
    id="++theme++cpskin.theme.base/less/mobile.less"
    enabled="0" />

 <stylesheet title="CPSkin LESS Medium Mobile Styles"
    id="++theme++cpskin.theme.base/less/mobilemedium.less"
    enabled="0" />

 <stylesheet title="CPSkin LESS Large Mobile Styles"
    id="++theme++cpskin.theme.base/less/mobilelarge.less"
    enabled="0" />


 <!-- Register newDream less files -->

 <stylesheet title="cpskin.diazotheme.newDream LESS Variables"
    id="++theme++cpskin.diazotheme.newDream/less/variables.less"
    media=""
    enabled="1"
    rendering="link"
    compression="none"
    expression="python:here.restrictedTraverse('@@currentTheme')() == 'cpskin.diazotheme.newDream'"
    applyPrefix="True"
    insert-before="portal_resources/cpskin/variables.less" />

 <stylesheet title="cpskin.diazotheme.newDream LESS Styles"
    id="++theme++cpskin.diazotheme.newDream/less/styles.less"
    media="all"
    enabled="1"
    rendering="link"
    compression="none"
    expression="python:here.restrictedTraverse('@@currentTheme')() == 'cpskin.diazotheme.newDream'"
    applyPrefix="True"
    insert-before="portal_resources/cpskin/styles.less" />

 <stylesheet title="CPSkin LESS Contact"
    id="++theme++cpskin.diazotheme.newDream/less/contact.less"
    media=""
    enabled="1"
    rendering="link"
    applyPrefix="True"
    expression="request/HTTP_X_THEME_ENABLED | nothing"
    compression="none"
    insert-after="++theme++cpskin.diazotheme.newDream/less/styles.less" />

 <stylesheet title="cpskin.diazotheme.newDream LESS Mobile Styles"
    id="++theme++cpskin.diazotheme.newDream/less/mobile.less"
    media="handheld, screen and (max-device-width: 480px), screen and (max-width: 480px)"
    enabled="1"
    rendering="link"
    applyPrefix="True"
    expression="python:here.restrictedTraverse('@@currentTheme')() == 'cpskin.diazotheme.newDream'"
    compression="none"
    insert-after="++theme++cpskin.diazotheme.newDream/less/contact.less" />

 <stylesheet title="cpskin.diazotheme.newDream LESS Medium Mobile Styles"
    id="++theme++cpskin.diazotheme.newDream/less/mobilemedium.less"
    media="handheld, screen and (min-device-width: 481px) and (max-device-width: 768px), screen and(min-width: 481px) and (max-width: 768px)"
    enabled="1"
    rendering="link"
    applyPrefix="True"
    expression="python:here.restrictedTraverse('@@currentTheme')() == 'cpskin.diazotheme.newDream'"
    compression="none"
    insert-after="++theme++cpskin.diazotheme.newDream/less/mobile.less" />

 <stylesheet title="cpskin.diazotheme.newDream LESS Large Mobile Styles"
    id="++theme++cpskin.diazotheme.newDream/less/mobilelarge.less"
    media="handheld, screen and (min-device-width: 769px) and (max-device-width: 1024px), screen and(min-width: 769px) and (max-width: 1024px)"
    enabled="1"
    rendering="link"
    applyPrefix="True"
    expression="python:here.restrictedTraverse('@@currentTheme')() == 'cpskin.diazotheme.newDream'"
    compression="none"
    insert-after="++theme++cpskin.diazotheme.newDream/less/mobilemedium.less" />

 <stylesheet title="cpskin.diazotheme.newDream LESS Full Mobile Styles"
    id="++theme++cpskin.diazotheme.newDream/less/mobilefull.less"
    media="handheld, screen and (min-device-width: 1025px), screen and(min-width: 1025px)"
    enabled="1"
    rendering="link"
    applyPrefix="True"
    expression="python:here.restrictedTraverse('@@currentTheme')() == 'cpskin.diazotheme.newDream'"
    compression="none"
    insert-after="++theme++cpskin.diazotheme.newDream/less/mobilelarge.less" />

</object>
